{
  "env": {
    "browser": true,
    "es2021": true,
    "node": true
  },
  "extends": ["eslint:recommended", "prettier"],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "rules": {
    "no-unused-vars": "warn",
    "no-console": "warn",
    "no-undef": "warn"
  },
  "globals": {
    "Chart": "readonly",
    "firebase": "readonly",
    "require": "readonly",
    "module": "readonly",
    "PlayerAnalytics": "readonly",
    "RecruitingHub": "readonly",
    "SmartGameInput": "readonly",
    "loadCoachDashboard": "readonly",
    "loadPlayerData": "readonly",
    "setDoc": "readonly"
  }
}
